{
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "QrCodeConfigurations": [
    {
      "PixelPerModule": "20",
      "QrCodeType": "1"
    },
    {
      "PixelPerModule": "20",
      "QrCodeType": "2"
    },
    {
      "PixelPerModule": "20",
      "QrCodeType": "3"
    }
  ]
}
